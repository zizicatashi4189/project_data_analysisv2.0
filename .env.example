# Supabase 配置
# 从 Supabase Dashboard > Settings > API 获取
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# 数据库连接（Supabase Dashboard > Settings > Database）
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.xxx.supabase.co:5432/postgres"

# Session 密钥（至少32个字符的随机字符串）
# 生成方法: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
SESSION_SECRET="your-random-32-character-secret-key"
